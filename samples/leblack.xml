<instance format="XCSP3" type="CSP">
  <variables>
    <array id="w" size="[2]"> 0..1 </array>
    <array id="y" size="[4]"> 0..3 </array>
  </variables>

  <constraints>
    <extension>
      <list> w[0] w[1] </list>
      <supports> (0,0) </supports>
    </extension>
  

  
    <extension>
      <list> w[1] y[1] </list>
      <supports> (1,1) </supports>
    </extension>
  

  
    <extension>
      <list> w[0] y[1] </list>
      <conflicts> (0,0) </conflicts>
    </extension>
  

  
    <extension>
      <list> y[3] y[3] </list>
      <supports> (0,2) (1,0) (2,2) </supports>
    </extension>
  </constraints>

</instance>
