      <extension>
        <list> %0 %1 </list>
        <supports> * </supports>
      </extension>
      <args> y[6] y[10] </args>
      <args> y[15] w[1] </args>
    </group>
   
    <group id="feo" >
      <extension>
        <list> %0 %1 </list>
        <supports> (0,0) </supports>
      </extension>
      <args> w[0] y[1] </args>
      <args> w[0] y[3] </args>
      <args> w[0] y[5] </args>
      <args> w[0] y[7] </args>
      <args> w[0] y[9] </args>
      <args> w[0] y[11] </args>
      <args> w[0] y[13] </args>
      <args> w[0] y[15] </args>
      <args> w[0] y[17] </args>
      <args> w[0] y[19] </args>
      <args> y[0] w[1] </args>
      <args> y[2] w[1] </args>
      <args> y[4] w[1] </args>
      <args> y[6] w[1] </args>
      <args> y[8] w[1] </args>
      <args> y[10] w[1] </args>
      <args> y[12] w[1] </args>
      <args> y[14] w[1] </args>
      <args> y[16] w[1] </args>
      <args> y[18] w[1] </args>
