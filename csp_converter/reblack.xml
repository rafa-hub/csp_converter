  <group id="Pero bueno" >
      <extension>
        <list> %0 %1 </list>
        <supports> (1,12) (1,11) (1,10) </supports>
      </extension>
      <args> w[0] y[2] </args>
      <args> w[0] y[4] </args>
      <args> w[0] y[6] </args>
      <args> y[1] w[1] </args>
      <args> y[3] w[1] </args>
      <args> y[5] w[1] </args>
    </group>

  
    
  </constraints>
</instance>
